{
  "name": "utils-better-check-dependencies",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/utils/better-check-dependencies/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "main": "libs/utils/better-check-dependencies/src/index.ts",
        "outputPath": "dist/libs/utils/better-check-dependencies",
        "outputFileName": "index.js",
        "format": ["esm"],
        "minify": false,
        "tsConfig": "libs/utils/better-check-dependencies/tsconfig.lib.json",
        "assets": [
          {
            "glob": "libs/utils/better-check-dependencies/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "platform": "node"
      },
      "configurations": {
        "development": {
          "minify": false
        },
        "production": {
          "minify": false
        }
      },
      "dependsOn": []
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/utils/better-check-dependencies/**/*.ts",
          "libs/utils/better-check-dependencies/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/libs/utils/better-check-dependencies"
      }
    }
  },
  "tags": []
}
