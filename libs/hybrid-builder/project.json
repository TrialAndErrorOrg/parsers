{
  "name": "hybrid-builder",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/hybrid-builder/src",
  "projectType": "library",
  "targets": {
    "make-publishable": {
      "executor": "better-nx-tsc:publishable"
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "rootDir": "libs/hybrid-builder/src",
        "outputPath": "dist/libs/hybrid-builder",
        "main": "libs/hybrid-builder/src/index.ts",
        "tsConfig": "libs/hybrid-builder/tsconfig.lib.json",
        "assets": [
          "libs/hybrid-builder/*.md",
          {
            "input": "./libs/hybrid-builder/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/hybrid-builder/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/hybrid-builder",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/hybrid-builder",
            "glob": "executors.json",
            "output": "."
          }
        ],
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/hybrid-builder/**/*.ts",
          "libs/hybrid-builder/generators.json",
          "libs/hybrid-builder/executors.json",
          "libs/hybrid-builder/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/hybrid-builder/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
