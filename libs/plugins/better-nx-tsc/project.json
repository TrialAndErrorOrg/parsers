{
  "name": "better-nx-tsc",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/plugins/better-nx-tsc/src",
  "projectType": "library",
  "targets": {
    "make-publishable": {
      "executor": "better-nx-tsc:publishable"
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/plugins/better-nx-tsc",
        "main": "libs/plugins/better-nx-tsc/src/index.ts",
        "tsConfig": "libs/plugins/better-nx-tsc/tsconfig.lib.json",
        "assets": [
          {
            "input": "./libs/plugins/better-nx-tsc/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/plugins/better-nx-tsc/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/plugins/better-nx-tsc",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/plugins/better-nx-tsc",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/plugins/better-nx-tsc/jest.config.ts"
      }
    }
  },
  "tags": []
}
