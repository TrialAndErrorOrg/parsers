{
  "name": "converter-frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/converter-frontend",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/apps/converter-frontend/.next"],
      "options": {
        "commands": [
          "next build apps/converter-frontend && mv apps/converter-frontend/.next/static apps/converter-frontend/.next/standalone/apps/converter-frontend/.next && cp -r apps/converter-frontend/public apps/converter-frontend/.next/standalone/apps/converter-frontend/.next"
        ]
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "converter-frontend:build",
        "dev": true
      },
      "configurations": {
        "production": {
          "buildTarget": "converter-frontend:build:production",
          "dev": false
        },
        "development": {
          "buildTarget": "converter-frontend:build:development",
          "dev": true
        }
      },
      "defaultConfiguration": "development"
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "converter-frontend:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/converter-frontend"],
      "options": {
        "jestConfig": "apps/converter-frontend/jest.config.js"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
