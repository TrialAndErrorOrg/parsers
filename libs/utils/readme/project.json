{
  "name": "utils-readme",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/utils/readme/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "rootDir": "libs/utils/readme/src",
        "outputPath": "dist/libs/utils/readme",
        "main": "libs/utils/readme/src/index.ts",
        "tsConfig": "libs/utils/readme/tsconfig.lib.json",
        "format": ["esm"],
        "skipTypeCheck": true,
        "assets": [
          "libs/utils/readme/*.md",
          {
            "input": "./libs/utils/readme/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/utils/readme/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/utils/readme",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/utils/readme",
            "glob": "executors.json",
            "output": "."
          }
        ],
        "generatePackageJson": true,
        "thirdParty": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/utils/readme/**/*.ts",
          "libs/utils/readme/generators.json",
          "libs/utils/readme/executors.json",
          "libs/utils/readme/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/utils/readme/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
