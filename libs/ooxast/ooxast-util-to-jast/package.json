{
  "name": "ooxast-util-to-jast",
  "version": "0.1.0",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/TrialAndErrorOrg/parsers",
  "author": "Thomas F. K. Jorna <hello@tefkah.com>",
  "type": "module",
  "description": "Util to convert `ooxast` syntax tree to `jast` syntax tree, allowing for `.docx` to `JATS XML` conversion.",
  "keywords": [
    "unified",
    "word",
    "docx",
    "office",
    "jats",
    "reoff",
    "rejour",
    "jats-xml",
    "ooxast"
  ],
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "ooxast-util-to-jast",
    "tsconfig": "./tsconfig.lib.json"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    }
  },
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "index.d.ts",
    "index.js",
    "/lib"
  ],
  "dependencies": {
    "ooxast": "0.3.0",
    "xast-util-is-element": "0.3.1",
    "ooxast-util-citations": "0.4.0",
    "jast-types": "0.1.2",
    "xast-util-select": "0.3.0",
    "ooxast-util-properties": "0.1.1",
    "jast-util-from-csl": "0.1.1",
    "reoff-parse": "0.5.0",
    "csl-json": "0.1.0",
    "xastscript": "^4.0.0",
    "unist-util-is": "^6.0.0",
    "rehype-minify-whitespace": "^6.0.0",
    "vfile": "^6.0.1",
    "@types/unist": "^3.0.0",
    "@types/xast": "^2.0.1"
  }
}
