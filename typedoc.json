{
  "entryPointStrategy": "packages",
  "tsconfig": "tsconfig.base.json",
  "symbolsWithOwnFile": "none",
  "entryPoints": [
    "libs/ooxast/ooxast-util-to-unified-latex",
    "libs/ooxast/ooxast-util-to-jast",
    "libs/ooxast/*",
    "libs/jast/*",
    "libs/reoff/*",
    "libs/rejour/*",
    "libs/citations/*",
    "libs/xast/*"
  ],
  "exclude": ["libs/citations/apa-parser", "libs/ooxast/ooxast-util-to-hast"],
  "hideBreadcrumbs": true,
  "hideInPageTOC": true,
  "gitRevision": "main",
  "kindSortOrder": [
    "Reference",
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "EnumMember",
    "Function",
    "Variable",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Accessor",
    "Method",
    "ObjectLiteral",
    "Parameter",
    "TypeParameter",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "GetSignature",
    "SetSignature"
  ]
}
