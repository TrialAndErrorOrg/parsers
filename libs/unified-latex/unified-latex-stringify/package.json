{
  "name": "unified-latex-stringify",
  "version": "0.0.1",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/TrialAndErrorOrg/unified-latex-stringify",
  "homepage": "https://github.com/TrialAndErrorOrg/parsers",
