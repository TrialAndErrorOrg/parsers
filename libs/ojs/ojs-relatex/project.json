{
  "name": "ojs-relatex",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ojs/ojs-relatex/src",
  "projectType": "library",
  "targets": {
    "readme": {
      "executor": "utils-readme:update-readme",
      "options": {
        "readme": "libs/ojs/ojs-relatex/README.md",
        "packageJSON": "libs/ojs/ojs-relatex/package.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/ojs/ojs-relatex/**/*.ts"]
      }
    },
    "build": {
      "executor": "better-nx-tsc:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "rootDir": "libs/ojs/ojs-relatex/src",
        "outputPath": "dist/libs/ojs/ojs-relatex",
        "tsConfig": "libs/ojs/ojs-relatex/tsconfig.lib.json",
        "packageJson": "libs/ojs/ojs-relatex/package.json",
        "main": "libs/ojs/ojs-relatex/src/index.ts",
        "assets": ["libs/ojs/ojs-relatex/*.md"],
        "updateBuildableProjectDepsInPackageJson": true,
        "clean": true,
        "buildableProjectDepsInPackageJsonType": "dependencies"
      },
      "dependsOn": [
        {
          "projects": "dependencies",
          "target": "build",
          "params": "forward"
        },
        {
          "projects": "self",
          "target": "lint"
        }
      ]
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    }
  },
  "tags": []
}
