{
  "name": "reoff-infer-headings",
  "version": "0.0.1",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/TrialAndErrorOrg/reoff-infer-headings",
  "homepage": "https://github.com/TrialAndErrorOrg/parsers",
  "author": "Thomas F. K. Jorna <hello@tefkah.com>",
  "type": "module",
  "description": "Plugin for `reoff` that turns a single bolded or emphasized line into a heading",
  "keywords": [
    "unified",
    "abstract",
    "syntax",
    "tree",
    "ast"
  ],
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "reoff-infer-headings",
    "tsconfig": "./tsconfig.lib.json"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    }
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "index.d.ts",
    "index.js",
    "/lib"
  ],
  "dependencies": {
    "reoff-markup-to-style": "0.2.0"
  }
}
