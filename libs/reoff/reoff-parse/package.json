{
  "name": "reoff-parse",
  "version": "0.0.1",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/TrialAndErrorOrg/parsers",
  "author": "Thomas F. K. Jorna <hello@tefkah.com>",
  "type": "module",
  "description": "Plugin for [reoff][reoff] to parse a `.docx` XML file into an `ooxast` AST. ",
  "keywords": [
    "unified",
    "reoff",
    "ooxast",
    "word",
    "docx"
  ],
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "reoff-parse",
    "tsconfig": "./tsconfig.lib.json"
  },
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./src/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "unified": "^10.1.2",
    "unist-util-filter": "^4.0.1",
    "xast": "^0.0.87",
    "xast-util-from-xml": "^3.0.0"
  }
}
