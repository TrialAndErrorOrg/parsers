  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "no-var": "warn",
        "@typescript-eslint/no-empty-interface": "warn"
