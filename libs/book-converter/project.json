{
  "name": "book-converter",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/book-converter/src",
  "projectType": "library",
  "targets": {
    "make-publishable": {
      "executor": "better-nx-tsc:publishable"
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "rootDir": "libs/book-converter/src",
        "outputPath": "dist/libs/book-converter",
        "tsConfig": "libs/book-converter/tsconfig.lib.json",
        "packageJson": "libs/book-converter/package.json",
        "main": "libs/book-converter/src/index.ts",
        "assets": ["libs/book-converter/*.md"],
        "clean": true
      },
      "dependsOn": [
        {
          "target": "build",
          "params": "forward",
          "dependencies": true
        },
        {
          "target": "lint"
        }
      ]
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/libs/book-converter"],
      "options": {
        "passWithNoTests": true
      }
    },
    "readme": {
      "executor": "utils-readme:update-readme",
      "options": {
        "readme": "libs/book-converter/README.md",
        "packageJSON": "libs/book-converter/package.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "fix": true,
        "hasTypeAwareRules": true
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "trackDeps": true,
        "push": true,
        "baseBranch": "main",
        "postTargets": [
          "book-converter:npm",
          "book-converter:github",
          "book-converter:github-standalone"
        ]
      },
      "trackDeps": true,
      "push": true,
      "baseBranch": "main"
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}"
      }
    },
    "github-standalone": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}",
        "repo": "TrialAndErrorOrg/reoff-cite"
      }
    },
    "npm": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    }
  },
  "tags": []
}
