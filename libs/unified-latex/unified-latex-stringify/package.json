    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "unified-latex-stringify",
    "tsconfig": "./tsconfig.lib.json"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    }
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "index.d.ts",
    "index.js",
    "./lib"
  ]
