{
  "name": "book-converter",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/book-converter/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "better-nx-tsc:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "rootDir": "libs/book-converter/src",
        "outputPath": "dist/libs/book-converter",
        "tsConfig": "libs/book-converter/tsconfig.lib.json",
        "packageJson": "libs/book-converter/package.json",
        "main": "libs/book-converter/src/index.ts",
        "assets": ["libs/book-converter/*.md"],
        "updateBuildableProjectDepsInPackageJson": true,
        "clean": true,
        "buildableProjectDepsInPackageJsonType": "dependencies"
      },
      "dependsOn": [
        {
          "projects": "dependencies",
          "target": "build",
          "params": "forward"
        },
        {
          "projects": "self",
          "target": "lint"
        }
      ]
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/libs/book-converter"],
      "options": {
        "passWithNoTests": true
      }
    },
    "readme": {
      "executor": "utils-readme:update-readme",
      "options": {
        "readme": "libs/book-converter/README.md",
        "packageJSON": "libs/book-converter/package.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/book-converter/**/*.ts"]
      }
    }
  },
  "tags": []
}
