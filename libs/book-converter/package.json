{
  "name": "@trialanderror/converter-cli",
  "version": "0.3.0",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/TrialAndErrorOrg/converter-cli",
  "homepage": "https://github.com/TrialAndErrorOrg/parsers",
  "author": "Thomas F. K. Jorna <hello@tefkah.com>",
  "description": "convert books",
  "type": "module",
  "bin": {
    "cote-conv": "./lib/bin/cli.js"
  },
  "keywords": [
    "unified",
    "word",
    "cli",
    "latex"
  ],
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "@trialanderror/converter-cli",
    "tsconfig": "./tsconfig.lib.json"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./public": {
      "types": "./public/schema.json",
      "import": "./public/schema.json"
    }
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "index.d.ts",
    "index.js",
    "/lib",
    "/public"
  ],
  "dependencies": {
    "yargs": "^17.7.2",
    "js-yaml": "^4.1.0",
    "chokidar": "^3.5.3",
    "zod": "^3.21.4",
    "ooxast-util-markup-to-style": "0.5.0",
    "unified": "^11.0.3",
    "reoff-unified-latex": "0.4.0",
    "unified-latex-stringify": "0.3.0",
    "docx-to-vfile": "0.11.0",
    "@unified-latex/unified-latex-builder": "^1.3.1",
    "reoff-clean": "0.4.0",
    "reoff-markup-to-style": "0.3.0",
    "reoff-parse-references": "0.4.0",
    "reoff-cite": "0.5.0",
    "xast-util-to-string": "^3.0.0",
    "ooxast": "0.4.0",
    "ooxast-util-get-style": "0.4.1",
    "@unified-latex/unified-latex-types": "^1.3.1",
    "zod-to-json-schema": "^3.21.4",
    "reoff-parse": "0.6.0",
    "ooxast-util-to-unified-latex": "0.6.0"
  }
}
