{
  "name": "rehype-notion",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/notion/rehype-notion/src",
  "projectType": "library",
  "targets": {
    "readme": {
      "executor": "@jote/utils/readme:update-readme",
      "options": {
        "readme": "libs/notion/rehype-notion/README.md",
        "packageJSON": "libs/notion/rehype-notion/package.json"
      }
    },
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/notion/rehype-notion",
        "tsConfig": "libs/notion/rehype-notion/tsconfig.lib.json",
        "packageJson": "libs/notion/rehype-notion/package.json",
        "main": "libs/notion/rehype-notion/src/index.ts",
        "assets": ["libs/notion/rehype-notion/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/notion/rehype-notion/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/notion/rehype-notion/jest.config.js",
        "passWithNoTests": true
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    }
  },
  "tags": []
}
